<h3 class="ui header">{{t "forms.i-i-s-kursovaya3-настрой-киоска-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya3-настрой-киоска-e.наличиеБумаги-caption"}}</label>
    {{flexberry-checkbox
      readonly=readonly
      value=model.наличиеБумаги
      class=(if (v-get validationModel "наличиеБумаги" "isInvalid") "error")
      data-test-i-i-s-kursovaya3-настрой-киоска-e-наличиеБумаги=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "наличиеБумаги" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya3-настрой-киоска-e.заправКартр-caption"}}</label>
    {{flexberry-checkbox
      readonly=readonly
      value=model.заправКартр
      class=(if (v-get validationModel "заправКартр" "isInvalid") "error")
      data-test-i-i-s-kursovaya3-настрой-киоска-e-заправКартр=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "заправКартр" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya3-настрой-киоска-e.датаНастройки-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаНастройки
      class=(if (v-get validationModel "датаНастройки" "isInvalid") "error")
      data-test-i-i-s-kursovaya3-настрой-киоска-e-датаНастройки=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаНастройки" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.времяНастройки
      class=(if (v-get validationModel "времяНастройки" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya3-настрой-киоска-e.времяНастройки-caption")
      data-test-i-i-s-kursovaya3-настрой-киоска-e-времяНастройки=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "времяНастройки" "message")}}
  </div>
</form>
